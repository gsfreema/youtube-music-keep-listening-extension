{
    "name": "YouTube Music Keep Listening",
    "description": "Keeps YouTube Music playing.",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://music.youtube.com/*"
            ],
            "js": [
                "listening-content-script.js"
            ]
        }
    ]
}